<p id="notice"><%= notice %></p>

<h1>ユーザー一覧</h1>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <ul class="user-list">
    <% @products.each do |product| %>
  <li>
   <%= image_tag (product.image.url) , class: "profile_img" %>
        <%= link_to product.name, product %>
        
      </li>
    <% end %>
  </ul>

<br>

